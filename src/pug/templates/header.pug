header.header
	.container
		.header-top.d-flex.align-items-center.mb-3.justify-content-between
			a.logo(href="/")
				img.mr-4(src="img/general/logo.png", alt="")
			.contacts-container.d-flex.flex-column.flex-sm-row.px-xl-4
				div.d-flex.flex-column.d-xl-block.mr-sm-5.mr-xl-0.mb-2.mb-sm-0
